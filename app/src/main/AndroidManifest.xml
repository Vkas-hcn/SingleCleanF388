        <activity android:name=".LiSingle"
            android:screenOrientation="portrait"
            android:launchMode="singleTop"
            android:exported="true"/>
        <activity android:name=".PiSingle"
            android:screenOrientation="portrait"
            android:launchMode="singleTop"
            android:exported="true"/>
        <activity android:name=".PengSingle"
            android:screenOrientation="portrait"
            android:launchMode="singleTop"
            android:exported="true"/>
        <activity android:name=".TanSingle"
            android:screenOrientation="portrait"
            android:launchMode="singleTop"
            android:exported="true"/>
        <activity-alias
            android:name="com.smoke.clears.away.single.DcSingle"
            android:enabled="@string/aligtGs11"
            android:exported="true"
            android:targetActivity=".QingSingle">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity-alias>
        <activity android:name=".DanSingle"
