                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
                <data
                    android:host="com.smoke.clears.away.single"
                    android:scheme="com.smoke.clears.away.single" />
            </intent-filter>
        </activity>
        <activity
            android:name=".d.c.DimoKeTing"
